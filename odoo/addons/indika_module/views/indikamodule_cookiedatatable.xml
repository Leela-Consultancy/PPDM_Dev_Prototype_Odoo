<?xml version="1.0" encoding="utf-8"?>
<odoo>
<act_window
id="indikamodule_cookiedatatable_action"
name="Cookie Type Data"
res_model="indikamodule.cookiedatatable" />

<menuitem
 id="indikamodule_cookiedatatable_menu"
 name="Cookie Type Data"
 action="indikamodule_cookiedatatable_action"
 parent="indikamodule_websitestable_menu"
 sequence="5" />

<record id ="indikamodule_cookiedatatable_view_form" model="ir.ui.view">
   <field name="name">Cookie Data Form</field>
   <field name="model">indikamodule.cookiedatatable</field>
   <field name="arch" type="xml">
         <form>
 	     <group>
	        <field name="cookie_name"/>
	     </group>
	 </form>
   </field>
</record>

<record id="indikamodule_cookiedatatable_view_tree" model="ir.ui.view">
  <field name="name">Cookie Data List</field>
  <field name="model">indikamodule.cookiedatatable</field>
  <field name="arch" type="xml">
    <tree>
      <field name="cookie_name"/>

    </tree>
    </field>
</record>	
	

</odoo>
